{
	"extends": "@react-native/typescript-config/tsconfig.json",
	"compilerOptions": {
		"target": "es5",
		"lib": ["es6", "dom"],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true
	},
	"include": ["node_modules/react-native/**/*"]
}
